# Aliases for dev containers
alias l='ls -lh'
alias ll='ls -lh'
alias la='ls -alh'
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'
alias :q='exit'

alias ga="git add"
alias gb="git branch -a"
alias gc="git commit -m"
alias gcb="git checkout -b"
alias gd="git diff"
alias gfap="git fetch --all --prune --tags"
alias gl="git log --oneline"
alias gla="git log --all --decorate --oneline --graph"
alias gn="git commit -n"
alias gp="git push"
alias gs="git status -s"
alias gu="git add -u"

# TODO: why is this necessary?
git config --global --add safe.directory /gsd
